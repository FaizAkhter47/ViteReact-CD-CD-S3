{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject",
        "AmazonS3FullAccess",
        "s3:PutObject"
      ],
      "Resource": [
        "arn:aws:s3:::node--project",
        "arn:aws:s3:::node--project/*"
      ]
    }
  ]
}
